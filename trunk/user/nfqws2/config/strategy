### http, https
--filter-tcp=80,443 --filter-l7=http,tls
--payload=http_req
--out-range=-d3
--lua-desync=hostfakesplit:tcp_md5:tcp_ts_up
--payload=tls_client_hello
--out-range=-d3
--lua-desync=multisplit:pos=1,midsld:seqovl=765:seqovl_pattern=tls_google:tcp_ts_up
<HOSTLIST>

